	Publisher = {G{\"o}ttingen University Press},
	Series = {G{\"o}ttingen Studies in Cultural Property},
	Title = {Negotiating Tradition: The Pragmatics of International Deliberations on Cul\-tural Property},
	Url = {http://resolver.sub.uni-goettingen.de/purl?isbn-978-3-86395-100-9},
	Volume = {4},
